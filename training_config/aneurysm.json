    "index": {
        "feature_index_start": 0,
        "feature_index_end": 16,
        "output_index_start": 0,
        "output_index_end": 5,
        "node_type_index": 16
    },
    "transformations": {
        "preprocessing": {
            "noise": [10.0, 0.1, 10.0, 0.1],
            "noise_index_start": [0, 3, 5, 8],
            "noise_index_end": [3, 4, 8, 9],
            "masking": 0
        },
        "world_pos_parameters": {
            "use": false,
            "world_pos_index_start": 0,
            "world_pos_index_end": 3
        }
    }
}
