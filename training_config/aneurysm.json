{
    "dataset": {
        "extension": "xdmf",
        "xdmf_folder": "/scratch-big/upelissier/50-Datasets/AnXplore/2d/stent/fixed/fivecycles/train",
        "meta_path": "dataset_config/aneurysm_meta.json",
        "targets": ["Vitesse", "CUP", "CAP", "PhiDBP"],
        "khop": 1,
        "new_edges_ratio": 0
    },
    "model": {
        "type": "epd",
        "message_passing_num": 10,
        "hidden_size": 64,
        "node_input_size": 16,
        "output_size": 5,
        "edge_input_size": 4
